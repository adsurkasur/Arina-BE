{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "build:server": "chmod +x node_modules/.bin/vite node_modules/.bin/esbuild || true && npx vite build && npx esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build": "npm run build:server",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^3.10.0",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@neondatabase/serverless": "^0.10.4",
    "better-sqlite3": "^11.10.0",
    "connect-pg-simple": "^10.0.0",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "esbuild": "^0.25.4",
    "express-session": "^1.18.1",
    "memorystore": "^1.6.7",
    "mongodb": "^6.16.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "vite": "^6.3.5",
    "ws": "^8.18.0"
  }
}
